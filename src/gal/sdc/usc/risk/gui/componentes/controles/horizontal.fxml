<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import com.jfoenix.controls.JFXButton?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="gal.sdc.usc.risk.gui.componentes.controles.ControlesController"
            maxWidth="Infinity" maxHeight="Infinity" minWidth="0" minHeight="0">
    <HBox AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
          AnchorPane.bottomAnchor="0.0" fx:id="parent">
        <HBox fx:id="contenedor" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" prefHeight="${parent.width * 90 / 100}"/>
        <HBox HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" prefHeight="${parent.width * 10 / 100}">
            <JFXButton onAction="#comandos">Comandos</JFXButton>
        </HBox>
    </HBox>
</AnchorPane>
